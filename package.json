		"build-gh": "parcel build src/index.html --out-dir dist --public-url /projets2/",
		"test": "echo \"Error: no test specified\" && exit 1",
		"lint": "eslint ./src --ext .js,.jsx,.ts,.tsx"
	},
	"publishConfig": {
		"registry": "https://npm.pkg.github.com/@Ashfurr"
	},
	"devDependencies": {
		"@typescript-eslint/eslint-plugin": "^2.29.0",
		"@typescript-eslint/parser": "^2.29.0",
		"eslint": "^6.8.0",
		"minimist": ">=1.2.2",
		"parcel-plugin-clean-easy": "^1.0.2",
		"parcel-plugin-static-files-copy": "^2.4.3",
		"typescript": "^3.8.3"
	},
	"dependencies": {
		"phaser": "^3.50.1"
	},
	"parcelCleanPaths": [
		"dist"
	],
